<?xml version="1.0" encoding="UTF-8"?>  
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" id="icons-onesignal-plugin" version="1.0.0"> 

    <name>Adicionar icons</name> 
    <description>Adiciona icones no android</description> 
    <license>MIT</license> 

    <engines> 
        <engine name="cordova" version=">=3.0.0" /> 
    </engines>  

    <!-- android --> 
    <platform name="android">
        
         <!-- drawable-hdpi-v11 -->
         <resource-file src="src/drawable-hdpi/ic_stat_onesignal_default.png" target="res/drawable-hdpi/ic_stat_onesignal_default.png" />

         <!--drawable-hdpi-v11 -->
         <resource-file src="src/drawable-hdpi-v11/ic_stat_onesignal_default.png" target="res/drawable-hdpi-v11/ic_stat_onesignal_default.png" />

         <!-- drawable-mdpi -->
         <resource-file src="src/drawable-mdpi/ic_stat_onesignal_default.png" target="res/drawable-mdpi/ic_stat_onesignal_default.png" />

         <!-- drawable-mdpi-v11 -->
         <resource-file src="src/drawable-mdpi-v11/ic_stat_onesignal_default.png" target="res/drawable-mdpi-v11/ic_stat_onesignal_default.png" />

         <!-- drawable-xhdpi -->
         <resource-file src="src/drawable-xhdpi/ic_stat_onesignal_default.png" target="res/drawable-xhdpi/ic_stat_onesignal_default.png" />

         <!-- drawable-xhdpi-v11 -->
         <resource-file src="src/drawable-xhdpi/ic_stat_onesignal_default.png" target="res/drawable-xhdpi-v11/ic_stat_onesignal_default.png" />

         <!-- drawable-xxhdpi -->
         <resource-file src="src/drawable-xxhdpi/ic_stat_onesignal_default.png" target="res/drawable-xxhdpi/ic_stat_onesignal_default.png" />

         <!-- drawable-xxhdpi-v11 -->
         <resource-file src="src/drawable-xxhdpi/ic_stat_onesignal_default.png" target="res/drawable-xxhdpi-v11/ic_stat_onesignal_default.png" />

         <!-- drawable-xxhdpi -->
         <resource-file src="src/drawable-xxxhdpi/ic_stat_onesignal_default.png" target="res/drawable-xxxhdpi/ic_stat_onesignal_default.png" />

         <!-- drawable-xxhdpi-v11 -->
         <resource-file src="src/drawable-xxxhdpi/ic_stat_onesignal_default.png" target="res/drawable-xxxhdpi-v11/ic_stat_onesignal_default.png" />                          
    </platform>  
    
</plugin>